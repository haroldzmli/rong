    "acorn": "^6.0.0",
    "webpack": "4.43.0",
    "mapbox-gl": "^0.53.1"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
